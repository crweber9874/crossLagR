@incollection{kenny_traitstate_2001,
	address = {Washington, DC, US},
	series = {Decade of behavior},
	title = {Trait–state models for longitudinal data},
	isbn = {978-1-55798-754-9},
	abstract = {States that over the past decade investigators have attempted to decompose variance in measures simultaneously into trait and state variance, and that there are currently two major approaches—the latent state–trait (LST) model as suggested by Steyer and colleagues, and the authors' own stable trait (ST), autoregressive trait (ART or AR), and state (S), or STARTS model (previously called the "trait–state–error model"). Both models partition variance in longitudinal data into trait and state components. In this chapter, the authors focus on these two models. Additionally, they show that the multitrait-multimethod matrix (MTMM; Campbell and Fiske, 1959) is relevant for the analysis of the matrix suggested by Kenny and Campbell (1989), that a preliminary MTMM analysis is helpful in determining how to proceed in the analysis of stability. These different models are evaluated in terms of the criteria: design, model conceptualization, model assumption plausibility, and estimation methods and difficulty. They illustrate each model's strengths and weakness through analysis of a secondary data set collected from 433 female adolescents measured 4 times at 6-mo intervals on the Center for Epidemiological Studies—Depression Scale. Comments by A. Davey on this chapter follow pp. 265–269. (PsycInfo Database Record (c) 2024 APA, all rights reserved)},
	booktitle = {New methods for the analysis of change},
	publisher = {American Psychological Association},
	author = {Kenny, David A. and Zautra, Alex},
	year = {2001},
	doi = {10.1037/10409-008},
	keywords = {Longitudinal Studies, Major Depression, Mathematical Modeling, Multivariate Analysis},
	pages = {243--263},
}

@article{kenny_trait-state-error_1995,
	title = {The trait-state-error model for multiwave data},
	volume = {63},
	issn = {0022-006X},
	doi = {10.1037//0022-006x.63.1.52},
	abstract = {Although researchers in clinical psychology routinely gather data in which many individuals respond at multiple times, there is not a standard way to analyze such data. A new approach for the analysis of such data is described. It is proposed that a person's current standing on a variable is caused by 3 sources of variance: a term that does not change (trait), a term that changes (state), and a random term (error). It is shown how structural equation modeling can be used to estimate such a model. An extended example is presented in which the correlations between variables are quite different at the trait, state, and error levels.},
	language = {eng},
	number = {1},
